<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
  <listeners>

   <listener class-name="ListenersPack.IListeners"></listener>

  </listeners>

  <test  name="Test_AmazonSprint1_Chrome">
  <parameter name="browser" value="chrome"></parameter>
  <parameter name="urladdress" value="https://www.amazon.com"></parameter>
    <classes>
      <class name="AmazonTestNG.Sprint1_Amazon"/>
    </classes>
  </test> <!-- Test_AmazonSprint1 -->
  
  <test name="Test_AmazonSprint1_edge">
  <parameter name="browser" value="edge"></parameter>
  <parameter name="urladdress" value="https://www.amazon.com"></parameter>
    <classes>
      <class name="AmazonTestNG.Sprint1_Amazon"/>
    </classes>
  </test>  
  
  
  <!--run in suite level means run all test in chrome thats why parameter in suite 
  run in different browser means run in test and parameter is in test -->
  
  
  <!-- here we are testing not parallely, 1 by 1 test chrome and edge -->
  
  
  
  
  
  
</suite> <!-- Suite -->
