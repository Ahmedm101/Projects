<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">


  <test thread-count="5" name="Testing SignUp data in chrome">
  <parameter name="browser" value="chrome"></parameter>
  <parameter name="urladdress" value="https://www.facebook.com"></parameter>
    <classes>
      <class name="FacebookTestNG.FacebookSignInTest"/>
    </classes>
  </test> <!-- Testing SignUp data -->
  
   <test thread-count="5" name="Testing SignUp data in edge">
  <parameter name="browser" value="edge"></parameter>
  <parameter name="urladdress" value="https://www.facebook.com"></parameter>
    <classes>
      <class name="FacebookTestNG.FacebookSignInTest"/>
    </classes>
  </test> <!-- Testing SignUp data -->
  
</suite> <!-- Suite -->
